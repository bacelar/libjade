require "indcpa.jinc"
from Jade require "crypto_kem/kyber/common/amd64/kem.jinc"

export fn jade_kem_kyber_kyber768_amd64_avx2_keypair(reg u64 pkp skp) -> reg u64
{
  reg u64 r;
  #msf reg u64 ms;

  _ = #init_msf();
  ms = __crypto_kem_keypair_jazz(pkp, skp);
  _, _, _, _, _, r = #set0();

  return r;
}


export fn jade_kem_kyber_kyber768_amd64_avx2_enc(reg u64 ctp shkp pkp) -> reg u64
{
  reg u64 r;
  #msf reg u64 ms;

  _ = #init_msf();
  ms = __crypto_kem_enc_jazz(ctp, shkp, pkp);
  _, _, _, _, _, r = #set0();

  return r;
}


export fn jade_kem_kyber_kyber768_amd64_avx2_dec(reg u64 shkp ctp skp) -> reg u64
{
  reg u64 r;
  #msf reg u64 ms;

  ms = #init_msf();
  ms = __crypto_kem_dec_jazz(shkp, ctp, skp, ms);
  _, _, _, _, _, r = #set0();

  return r;
}
